{{#each books as | book |}}
<div class="book-card-1" id = 'book-info'>
    <div class="book-card-header" id = 'title'>
        {{book.dataValues.title}}
    </div>
    <div class="card-body">
        <p>
            Author:
            {{book.dataValues.author}}
            Pages:
            {{book.dataValues.pages}}
            ISBN:
            {{book.dataValues.isbn}}
            Book Cover:
            <a href = '{{book.dataValues.thumbnail}}'><img src = '{{book.dataValues.thumbnail}}'></a>
        </p>
     </div>
     <button class = 'delete-button' bID = {{book.dataValues.id}}>Delete</button>
</div>
{{/each}}
<script src="js/delete.js"></script>
